{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "docs/(.*)",
            "dest": "api/index.py"
        }
    ]
}